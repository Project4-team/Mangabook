@using SachOnline.Models;
@model List<Sach>
@{
    ViewBag.Title = "SachTheoTheloai";
}

@{
    if (Model.Count == 0)
    {
       <h1>@ViewBag.ChuDe</h1>
    }
    foreach (var item in Model)
    {
        <div>@item.MaSach</div>
        <div>@item.TenSach</div>
        <div>@item.GiaBan</div>
    }
}

